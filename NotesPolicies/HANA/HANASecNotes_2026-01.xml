  <!-- SAP HANA -->
  <!-- [p2-CVSS 8.8] HAN-DB-SEC 0003691059 - [CVE-2026-0492] Privilege escalation vulnerability in SAP HANA database (Version 7) -->
  <configstore name="HDB_VERSION">
    <checkitem desc="[p2-CVSS 8.8] Note 3691059v7 requires HANA revision update" id="0003691059" not_found="positive" system_attributes="DB_REPL_OP_MODE:!LOGREPLAY">
      <compliant>
                 (  
                 (NAME = 'VERSION' AND substring(VALUE,0,7) = '2.00.07' AND substring(VALUE,0,11) &gt;= '2.00.079.07')
				 OR (NAME = 'VERSION' AND substring(VALUE,0,7) = '2.00.08' AND substring(VALUE,0,8) &gt;= '2.00.088')
                 OR (NAME = 'VERSION' AND substring(VALUE,0,7) &gt;= '2.00.09' )
				 OR (NAME = 'VERSION' AND substring(VALUE,0,7) = '2.00.06' )
				 OR (NAME = 'VERSION' AND substring(VALUE,0,7) = '2.00.05' ))  
			</compliant>
      <noncompliant> 
                 ( 
				 (NAME = 'VERSION' AND substring(VALUE,0,7) = '2.00.07' AND NOT substring(VALUE,0,11) &gt;= '2.00.079.07')
                 OR (NAME = 'VERSION' AND substring(VALUE,0,7) = '2.00.08' AND NOT substring(VALUE,0,8) &gt;= '2.00.088')
				 OR (NAME = 'VERSION' AND substring(VALUE,0,7) &lt; '2.00.05')
				 )
     </noncompliant>
    </checkitem>
  </configstore>
